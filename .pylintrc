[MASTER]
ignore=venv

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name

[FORMAT]
max-line-length=88

[DESIGN]
max-args=5
max-locals=15
max-returns=6
max-branches=12
max-statements=50
max-parents=7
max-attributes=7
max-public-methods=20

[LOGGING]
logging-modules=logging

[IMPORTS]
known-standard-library=collections,os,sys
known-third-party=flask,sqlalchemy

[TYPECHECK]
generated-members=sqlalchemy.*
